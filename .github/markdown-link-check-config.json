{
  "ignorePatterns": [
    {
      "pattern": "^http://localhost"
    },
    {
      "pattern": "^https://127.0.0.1"
    },
    {
      "pattern": "^http://192.168"
    },
    {
      "pattern": "^https://192.168"
    },
    {
      "pattern": "^http://10\\."
    },
    {
      "pattern": "^https://10\\."
    }
  ],
  "replacementPatterns": [
    {
      "pattern": "^https://openai.com/gpt-4$",
      "replacement": "https://openai.com/product/gpt-4"
    }
  ],
  "httpHeaders": [
    {
      "urls": ["https://www.terraform.io", "https://www.hashicorp.com", "https://terraform.io", "https://hashicorp.com"],
      "headers": {
        "User-Agent": "Mozilla/5.0 (X11; Linux x86_64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/91.0.4472.124 Safari/537.36",
        "Accept": "text/html,application/xhtml+xml,application/xml;q=0.9,image/webp,*/*;q=0.8",
        "Accept-Language": "en-US,en;q=0.5",
        "Accept-Encoding": "gzip, deflate",
        "Connection": "keep-alive"
      }
    },
    {
      "urls": ["https://openai.com"],
      "headers": {
        "User-Agent": "Mozilla/5.0 (X11; Linux x86_64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/91.0.4472.124 Safari/537.36",
        "Accept": "text/html,application/xhtml+xml,application/xml;q=0.9,image/webp,*/*;q=0.8"
      }
    }
  ],
  "timeout": "30s",
  "retryOn429": true,
  "retryCount": 3,
  "fallbackHttpStatus": [400, 401, 403, 404, 429, 500, 502, 503, 504],
  "aliveStatusCodes": [200, 201, 202, 203, 204, 205, 206, 300, 301, 302, 303, 304, 307, 308],
  "ignoreHttpsErrors": false,
  "noWeb": false
}
